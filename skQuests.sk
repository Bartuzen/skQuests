local expression skquests placeholder[s] parsed %$strings% [(for|from) %-player%]:
	return type: string
	get:
		Bukkit.getPluginManager().isPluginEnabled("PlaceholderAPI"):
			loop expressions 1:
				add PlaceholderAPI.setPlaceholders(expression 2, loop-value) to {_return::*}
			return {_return::*}
		else:
			return expression 1
local expression %item% with skquests lore %$strings%:
	return type: item
	get:
