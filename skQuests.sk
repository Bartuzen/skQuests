function skQuests_YAMLParse() :: objects:
	yaml nodes with keys "" from "skQuests-Quests" isn't set:
		add "&7[&askQuests&7] &c%yaml value ""parsing.error"" from ""skQuests-Lang""%: %yaml value ""parsing.category.no-category"" from ""skQuests-Lang""%" to {_return::*}
	loop yaml nodes with keys "" from "skQuests-Quests":
		yaml value "%loop-value%.name" from "skQuests-Quests" isn't set:
			add "&7[&askQuests&7] &c%yaml value ""parsing.error"" from ""skQuests-Lang""%: %yaml value ""parsing.category.name"" from ""skQuests-Lang""% (%loop-value%)" to {_return::*}
		yaml value "%loop-value%.icon" from "skQuests-Quests" isn't set:
			add "&7[&askQuests&7] &c%yaml value ""parsing.error"" from ""skQuests-Lang""%: %yaml value ""parsing.category.icon"" from ""skQuests-Lang""% (%loop-value%)" to {_return::*}
		else if yaml value "%loop-value%.icon" from "skQuests-Quests" parsed as item isn't set:
			set {_t} to yaml value "parsing.quest.unknown.item" from "skQuests-Lang"
			replace "%%item%%" with yaml value "%loop-value%.icon" from "skQuests-Quests" in {_t}
			add "&7[&askQuests&7] &c%yaml value ""parsing.error"" from ""skQuests-Lang""%: %{_t}% (%loop-value-1%.icon)" to {_return::*}
		yaml nodes with keys "%loop-value%.quests" from "skQuests-Quests" isn't set:
			add "&7[&askQuests&7] &c%yaml value ""parsing.error"" from ""skQuests-Lang""%: %yaml value ""parsing.category.quest"" from ""skQuests-Lang""% (%loop-value%)" to {_return::*}
		loop yaml nodes with keys "%loop-value%.quests" from "skQuests-Quests":
			yaml value "%loop-value-1%.quests.%loop-value-2%.type" from "skQuests-Quests" isn't set:
				add "&7[&askQuests&7] &c%yaml value ""parsing.error"" from ""skQuests-Lang""%: %yaml value ""parsing.quest.type"" from ""skQuests-Lang""% (%loop-value-1%.quests.%loop-value-2%)" to {_return::*}
			else if yaml value "%loop-value-1%.quests.%loop-value-2%.type" from "skQuests-Quests" isn't "kill", "mobkill", "mob_kill", "mob kill", "break", "blockbreak", "block_break", "block break", "retrieve", "collect", "place", "blockplace", "block_place", "craft", "itemcraft", "item_craft", "item craft", "smelt", "itemsmelt", "item_smelt" or "item smelt":
				set {_t} to yaml value "parsing.quest.unknown.type" from "skQuests-Lang"
				replace "%%type%%" with yaml value "%loop-value-1%.quests.%loop-value-2%.type" from "skQuests-Quests" in {_t}
				add "&7[&askQuests&7] &c%yaml value ""parsing.error"" from ""skQuests-Lang""%: %{_t}% (%loop-value-1%.quests.%loop-value-2%)" to {_return::*}
			else:
				yaml value "%loop-value-1%.quests.%loop-value-2%.type" from "skQuests-Quests" is "retrieve", "collect", "break", "blockbreak", "block_break", "block break", "place", "blockplace", "block_place", "block place", "craft", "itemcraft", "item_craft", "item craft", "smelt", "itemsmelt", "item_smelt" or "item smelt":
					loop yaml list "%loop-value-1%.quests.%loop-value-2%.requirements" from "skQuests-Quests":
						set {_l} to "%loop-value-3%"
						set {_l::*} to {_l} split at " as "
						{_l::1} parsed as item isn't set
						set {_t} to yaml value "parsing.quest.unknown.item" from "skQuests-Lang"
						replace "%%item%%" with "%loop-value-3%" in {_t}
						add "&7[&askQuests&7] &c%yaml value ""parsing.error"" from ""skQuests-Lang""%: %{_t}% (%loop-value-1%.quests.%loop-value-2%.requirements)" to {_return::*}
				else:
					loop yaml list "%loop-value-1%.quests.%loop-value-2%.requirements" from "skQuests-Quests":
						set {_l} to "%loop-value-3%"
						set {_l::*} to {_l} split at " as "
						{_l::1} parsed as entity type is set:
							set {_mob} to {_l::1} parsed as entity type
						else:
							set {_l2::*} to {_l::1} split at " "
							{_l2::1} parsed as integer is set
							loop size of {_l2::*} times:
								loop-number isn't 1
								add {_l2::%loop-number%} to {_mob::*}
							set {_mob} to join {_mob::*} with " " parsed as entity type
						{_mob} isn't set
						set {_t} to yaml value "parsing.quest.unknown.entity" from "skQuests-Lang"
						replace "%%entity%%" with "%loop-value-3%" in {_t}
						add "&7[&askQuests&7] &c%yaml value ""parsing.error"" from ""skQuests-Lang""%: %{_t}% (%loop-value-1%.quests.%loop-value-2%.requirements)" to {_return::*}
			yaml value "%loop-value-1%.quests.%loop-value-2%.name" from "skQuests-Quests" isn't set:
				add "&7[&askQuests&7] &c%yaml value ""parsing.error"" from ""skQuests-Lang""%: %yaml value ""parsing.quest.name"" from ""skQuests-Lang""% (%loop-value-1%.quests.%loop-value-2%)" to {_return::*}
			yaml value "%loop-value-1%.quests.%loop-value-2%.icon" from "skQuests-Quests" isn't set:
				add "&7[&askQuests&7] &c%yaml value ""parsing.error"" from ""skQuests-Lang""%: %yaml value ""parsing.quest.icon"" from ""skQuests-Lang""% (%loop-value-1%.quests.%loop-value-2%)" to {_return::*}
			else if yaml value "%loop-value-1%.quests.%loop-value-2%.icon" from "skQuests-Quests" parsed as item isn't set:
				set {_t} to yaml value "parsing.quest.unknown.item" from "skQuests-Lang"
				replace "%%item%%" with yaml value "%loop-value-1%.quests.%loop-value-2%.icon" from "skQuests-Quests" in {_t}
				add "&7[&askQuests&7] &c%yaml value ""parsing.error"" from ""skQuests-Lang""%: %{_t}% (%loop-value-1%.quests.%loop-value-2%.icon)" to {_return::*}
			yaml value "%loop-value-1%.quests.%loop-value-2%.reset-in" from "skQuests-Quests" is set:
				yaml value "%loop-value-1%.quests.%loop-value-2%.reset-in" from "skQuests-Quests" parsed as timespan isn't set
				set {_t} to yaml value "parsing.quest.unknown.reset-in" from "skQuests-Lang"
				replace "%%timespan%%" with yaml value "%loop-value-1%.quests.%loop-value-2%.reset-in" from "skQuests-Quests" in {_t}
				add "&7[&askQuests&7] &c%yaml value ""parsing.error"" from ""skQuests-Lang""%: %{_t}% (%loop-value-1%.quests.%loop-value-2%)" to {_return::*}
			yaml list "%loop-value-1%.quests.%loop-value-2%.requirements" from "skQuests-Quests" isn't set:
				add "&7[&askQuests&7] &c%yaml value ""parsing.error"" from ""skQuests-Lang""%: %yaml value ""parsing.quest.requirements"" from ""skQuests-Lang""% (%loop-value-1%.quests.%loop-value-2%)" to {_return::*}
			loop yaml list "%loop-value-1%.quests.%loop-value-2%.complete.items" from "skQuests-Quests":
				set {_l} to "%loop-value-3%"
				set {_l::*} to {_l} split at " as "
				{_l::1} parsed as item isn't set
				set {_t} to yaml value "parsing.quest.unknown.item" from "skQuests-Lang"
				replace "%%item%%" with "%loop-value-3%" in {_t}
				add "&7[&askQuests&7] &c%yaml value ""parsing.error"" from ""skQuests-Lang""%: %{_t}% (%loop-value-1%.quests.%loop-value-2%.requirements)" to {_return::*}
			yaml value "%loop-value-1%.quests.%loop-value-2%.complete.money" from "skQuests-Quests" is set:
				set {_m} to yaml value "%loop-value-1%.quests.%loop-value-2%.complete.money" from "skQuests-Quests"
				"%{_m}%" parsed as number isn't set
				set {_t} to yaml value "parsing.quest.money-number" from "skQuests-Lang"
				replace "%%money%%" with "%{_m}%" in {_t}
				add "&7[&askQuests&7] &c%yaml value ""parsing.error"" from ""skQuests-Lang""%: %{_t}% (%loop-value-1%.quests.%loop-value-2%.complete.money)" to {_return::*}
	{_return::*} isn't set:
		add "&7[&askQuests&7] &a%yaml value ""parsing.successful"" from ""skQuests-Lang""%" to {_return::*}
	else:
		set {_t} to "%yaml value ""parsing.not-successful"" from ""skQuests-Lang""%"
		replace "%%errors%%" with "&l&c%size of {_return::*}%&c" in {_t}
		add "&7[&askQuests&7] &c%{_t}%" to {_return::*}
	return {_return::*}
function skQuests_checkUpdate() :: text:
	set {_in} to new BufferedReader(new InputStreamReader(new URL("https://raw.githubusercontent.com/Bartuzen/skQuests/master/skQuests.sk").openStream()))
	while 1 is 1:
		add 1 to {_count}
		set {_return::%{_count}%} to {_in}.readLine()
		{_return::%{_count}%} isn't set:
			{_in}.close()
			return {_none}
		else if {_count} is 2:
			{_in}.close()
			replace " ", "	" and "version:" with "" in {_return::%{_count}%}
			return {_return::%{_count}%}
function skQuests_roundedTimespan(t: timespan) :: text:
	loop split "%{_t}%" at " ":
		loop-value parsed as number is set:
			add round(loop-value parsed as number) to {_return::*}
		else if loop-value isn't "and":
			last 1 character of loop-value is "s":
				add yaml value "time.plural.%loop-value%" from "skQuests-Lang" to {_return::*}
			else:
				add yaml value "time.single.%loop-value%" from "skQuests-Lang" to {_return::*}
	return join {_return::*} with " "
function skQuests_getPlayer(p: player) :: text:
	yaml value "use-uuid" from "skQuests-Config" is true:
		return {_p}'s uuid
	else:
		return {_p}'s name in lower case
function skQuests_sendTitle(p: player, q: string):
	set {_t} to yaml value "title.quest-completed" from "skQuests-Lang"
	replace "%%quest%%" with yaml value "%first element out of {_q} split at "".""%.quests.%last element out of {_q} split at "".""%.name" from "skQuests-Quests" in {_t}
	try {_p}.sendTitle("&a%yaml value ""title.completed"" from ""skQuests-Lang""%",placeholder parsed "&a%{_t}%" from {_p},("%yaml value ""complete.title.fade-in"" from ""skQuests-Config""%" parsed as integer otherwise 10),("%yaml value ""complete.title.stay"" from ""skQuests-Config""%" parsed as integer otherwise 70),("%yaml value ""complete.title.fade-out"" from ""skQuests-Config""%" parsed as integer otherwise 20))
	play sound yaml value "complete.sound.sound" from "skQuests-Config" with volume ("%yaml value ""complete.sound.volume"" from ""skQuests-Config""%" parsed as integer otherwise 1) with pitch ("%yaml value ""complete.sound.pitch"" from ""skQuests-Config""%" parsed as integer otherwise 1) at {_p} for {_p}
function skQuests_progress(p: player, block: item, types: strings):
	loop yaml nodes with keys "" from "skQuests-Quests":
		loop yaml nodes with keys "%loop-value%.quests" from "skQuests-Quests":
			delete {_ok}
			loop yaml list "%loop-value-1%.quests.%loop-value-2%.quests" from "skQuests-Quests":
				yaml value "%loop-value-3%.completed" from "skQuests-Player-%skQuests_getPlayer({_p})%" isn't set
				set {_ok} to 1
			yaml value "%loop-value-1%.quests.%loop-value-2%.reset-in" from "skQuests-Quests" is set:
				yaml value "%loop-value-1%.%loop-value-2%.reset" from "skQuests-Player-%skQuests_getPlayer({_p})%" is set
				set {_date::*} to yaml value "%loop-value-1%.%loop-value-2%.reset" from "skQuests-Player-%skQuests_getPlayer({_p})%" split at " "
				difference between now and date({_date::1} parsed as integer, {_date::2} parsed as integer, {_date::3} parsed as integer, {_date::4} parsed as integer, {_date::5} parsed as integer, {_date::6} parsed as integer) is smaller than yaml value "%loop-value-1%.quests.%loop-value-2%.reset-in" from "skQuests-Quests" parsed as timespan
				set {_ok} to 1
			{_ok} isn't set
			yaml value "%loop-value-1%.%loop-value-2%.completed" from "skQuests-Player-%skQuests_getPlayer({_p})%" isn't set
			{_types::*} contains yaml value "%loop-value-1%.quests.%loop-value-2%.type" from "skQuests-Quests"
			loop yaml list "%loop-value-1%.quests.%loop-value-2%.requirements" from "skQuests-Quests":
				set {_l} to "%loop-value-3%"
				set {_l::*} to {_l} split at " as "
				{_l::1} parsed as item is set
				{_block} is "%{_l::1}%" parsed as itemtype
				yaml value "%loop-value-1%.%loop-value-2%.progress.%1 of {_l::1} parsed as item%" from "skQuests-Player-%skQuests_getPlayer({_p})%" is set:
					first element out of {_l::1} split at " " parsed as integer is bigger than yaml value "%loop-value-1%.%loop-value-2%.progress.%1 of {_l::1} parsed as item%" from "skQuests-Player-%skQuests_getPlayer({_p})%"
					yaml value "%loop-value-1%.%loop-value-2%.progress.%1 of {_l::1} parsed as item%" from "skQuests-Player-%skQuests_getPlayer({_p})%" + item amount of {_block} is bigger than first element out of {_l::1} split at " " parsed as integer:
						set yaml value "%loop-value-1%.%loop-value-2%.progress.%1 of {_l::1} parsed as item%" from "skQuests-Player-%skQuests_getPlayer({_p})%" to first element out of {_l::1} split at " " parsed as integer
						skQuests_sendTitle({_p},"%loop-value-1%.%loop-value-2%")
					else:
